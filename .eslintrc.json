{
    "env": {
        "node": true,
        "es6": true,
        "jest": true
    },
    "extends": "eslint:recommended",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 4],
        "linebreak-style": ["error", "windows"],
        "quotes": ["error", "single"],
        "semi": ["error", "always"],
        "no-multiple-empty-lines": [
            "error",
            {
                "max": 1
            }
        ],
        "no-console": 0,
        "no-prototype-builtins": 0,
        "no-unused-vars": 0
    }
}
